{
  "name": "fitsync-ai-backend",
  "version": "1.0.0",
  "description": "FitSync AI Platform - Backend API Services",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc && npm run copy-files",
    "start": "node dist/server.js",
    "copy-files": "cp -r src/public dist/ 2>/dev/null || xcopy /E /I src\\public dist\\public 2>nul || true",
    "railway:deploy": "npm run build && npm start",
    "render:deploy": "npm run build && npm start",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "redis": "^4.6.12",
    "bull": "^4.12.2",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "socket.io": "^4.7.4",
    "nodemailer": "^6.9.7",
    "@tensorflow/tfjs-node": "^4.15.0",
    "openai": "^4.20.1",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "fitness",
    "ai",
    "health",
    "api",
    "backend"
  ],
  "author": "FitSync AI Team",
  "license": "MIT"
}